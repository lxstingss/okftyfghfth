<Window x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Discord_IP_Puller"
        mc:Ignorable="d"
        Title="Discord IP Puller" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="47*"/>
            <ColumnDefinition Width="353*"/>
        </Grid.ColumnDefinitions>
        <Label Content="Discord IP Puller" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" Height="59" Width="300" FontSize="36" Grid.ColumnSpan="2"/>
        <Button Content="Pull IP of User" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="87" Margin="10,190,0,0" VerticalAlignment="Top" Width="237" Click="Button_Click"/>
        <TextBox Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="23" Margin="10,135,0,0" Text="User Tag" TextWrapping="Wrap" VerticalAlignment="Top" Width="237"/>
    </Grid>
</Window>
